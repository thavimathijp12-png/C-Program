#include<stdio.h>
int main ()
{
    int s = 7;
    int t =1 ;
    for (int i = 1; i <= s; i++ )
        {
        for (int j = 1; j<= i; j++)
            {
            printf("%d",t);
            if (t == 1)
            {
                t = 0;
            }
            else {
                t = 1;
            }
        }
        printf("\n");
    }
    return 0;
}
