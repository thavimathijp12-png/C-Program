#include <stdio.h>
int main() {
    int N, original, sum = 0, rem;
    scanf("%d", &N);
    original = N;
    for (int temp = N; temp > 0; temp /= 10) {
        rem = temp % 10;
        sum = sum + (rem * rem * rem);
    }
    if (sum == original) printf("Yes\n");
    else printf("No\n");
    return 0;
}
