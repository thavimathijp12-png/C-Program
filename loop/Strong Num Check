#include <stdio.h>
int main() {
    int N, original_N, last_digit, sum = 0;
    scanf("%d", &N);
    original_N = N;
    while (N > 0) {
        last_digit = N % 10;
        int factorial = 1;
        for (int i = 1; i <= last_digit; i++) {
            factorial = factorial * i;
        }
        sum = sum + factorial;
        N = N / 10;
    }
    if (sum == original_N) {
        printf("Yes\n");
    } else
    {
        printf("No\n");
    }
    return 0;
}
