#include <stdio.h>
int main() {
    int N;
    int vibrat;
    int i = 1;

    int unsafe = 0;
    int consecutive = 0;
    int breakdown= -1;

    scanf("%d", &N);

    while (i <= N) {
        scanf("%d", &vibrat);

        if (vibrat > 70) {
            unsafe++;
            consecutive++;
            if (consecutive== 3 && breakdown == -1) {
                breakdown = i;
            }
        } else {
            consecutive = 0;
        }

        i++;
    }

    if (breakdown != -1)
        printf("Breakdown At Reading: %d\n", breakdown);
    else
        printf("Breakdown At Reading: Not Occurred\n");

    printf("Unsafe Readings: %d\n", unsafe);

    return 0;
}
