#include <stdio.h>
int main() {
    int total;
    int n,use;
    int  i=0;
    scanf("%d",&total);
    scanf("%d",&n);
    while (i < n && total > 0 ) {
        scanf("%d",&use);
        total = total - use;
        i++;
    }
    if (total < 0) {
        total = 0;
    }
    printf("Days Used:%d\n",i);
    printf("Remaining Data :%dGB",total);
    return 0;

}
