#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    int large=0;
    int sec=0;
    for(int i=0;i<n;i++) {
        if (arr[i]>0) {
            sec=large;
            large = arr[i];
        }
        else if (arr[i]>sec&&arr[i]<large) {
            sec=arr[i];
        }
    }
    printf("%d",sec);
    return 0;
}
