#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int st[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &st[i]);
    }
    int left = 0;
    int right = n - 1;
    while (left < right) {
        int temp = st[left];
        st[left] = st[right];
        st[right] = temp;
        left++;
        right--;
    }
    for (int i = 0; i < n; i++) {
        printf("%d ", st[i]);
    }

    return 0;
}
