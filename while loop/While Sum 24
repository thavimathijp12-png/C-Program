#include <stdio.h>
int main() {
    int fuel, N;
    int burn;
    int stage = 1;
    int emergency = -1;
    int Wasted = 0;

    scanf("%d", &fuel);

    scanf("%d", &N);

    while (stage <= N) {
        scanf("%d", &burn);

        fuel -= burn;

        if (fuel < 0 && emergency == -1) {
            emergency = stage;
            Wasted = -fuel;
        }

        stage++;
    }

    if (emergency!= -1)
        printf("Emergency Stage: %d\n", emergency);
    else
        printf("Emergency Stage: Not Occurred\n");

    printf("Fuel Wasted: %d\n", Wasted);

    return 0;
}
