#include<stdio.h>
int main() {
    int N;
    int attempt;
    int i = 1;
    int consecutive = 0;
    int total = 0;
    int lock = -1;

    scanf("%d", &N);
    while (i <= N) {
        scanf("%d", &attempt);
        if (attempt == 0) {
            total++;
            consecutive++;
            if (consecutive == 3 && lock == -1) {
                lock = i;
            }
        } else {
            consecutive = 0;
        }

        i++;
    }

    if (lock != -1)
        printf("Lock Triggered At Attempt: %d\n", lock);
    else
        printf("Lock Triggered At Attempt: Not Locked\n");

    printf("Total Failed Attempts: %d\n", total);

    return 0;
}
