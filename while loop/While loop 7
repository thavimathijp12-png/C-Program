#include <stdio.h>
int main()
{
    int capacity, n;
    int change;
    int occupied = 0;
    int criticalHours = 0;
    int i = 0;
    float threshold;
    scanf("%d", &capacity);
    scanf("%d", &n);

    threshold = capacity * 0.9;
    while (i < n)
    {
        scanf("%d", &change);

        occupied += change;

        if (occupied > threshold)
        {
            criticalHours++;
        }

        i++;
    }
    printf("Final Occupied Beds: %d\n", occupied);
    printf("Critical Hours: %d\n", criticalHours);

    return 0;
}
